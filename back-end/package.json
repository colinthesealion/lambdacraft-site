{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "back-end",
  "author": "Colin Barringer",
  "module": "dist/back-end.esm.js",
  "size-limit": [
    {
      "path": "dist/back-end.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/back-end.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ftp": "^0.3.31",
    "@types/prompt-sync": "^4.1.0",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "basic-ftp": "^4.6.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "prompt-sync": "^4.2.0",
    "regenerator-runtime": "^0.13.7"
  }
}
